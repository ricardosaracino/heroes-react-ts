{
  "displayAsWarning": true,

  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],

  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": [true, "allow-leading-underscore"]
  },

  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}